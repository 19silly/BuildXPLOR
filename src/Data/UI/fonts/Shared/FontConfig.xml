<!-- 
Each FontMap entry will map a system font to its exported font in fonts_en with the appropriate style flag 
FontMap attributes:
	fontName: the real name of the font on your system
	ASLinkage: the AS linkage name specified for the font in the fonts_en.fla library
	fontFlag: the style flag of the font (1 max), mapped to GFxFontMap::MapFontFlags
		Flag options: Normal, Italic, Bold, BoldItalic, FauxItalic, FauxBold, FauxBoldItalic
-->

<FontConfig>

  <FontMap fontName="Blender Pro Medium" ASLinkage="$Text1Med" fontFlag="Normal" />
  <FontMap fontName="Blender Pro Bold" ASLinkage="$Text1Bold" fontFlag="Bold" />
  <FontMap fontName="Jura Medium" ASLinkage="$Title1Med" fontFlag="Normal" />
  <FontMap fontName="CapsuulaRegular" ASLinkage="$Condensed" fontFlag="Normal" />
  
</FontConfig>