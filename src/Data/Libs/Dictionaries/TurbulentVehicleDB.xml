<Dictionary>
<!-- All entries that can map a Turbulent item code
	<Entry key="TURBULENT-ITEM-CODE">
		<Element value="string:Item class" comment="Any useful comment"/>
		<Element value="int:Item type" comment="Any useful comment" />
		<Element value="int:Item Subtype" comment="Any useful comment" />
		<Element value="string:Player Loadout" comment="Any useful comment" />
	</Entry>
	
	Sub types:
		value="0" -TNGS Ship
		value="1" -HangarReady Ship
		value="2" -Buggy
		value="3" -WeaponMount
		value="4" -DFM Trainer Ship
		value="5" -FlightReady Ship
!-->

