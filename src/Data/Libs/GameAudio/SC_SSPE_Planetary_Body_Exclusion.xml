<ATLConfig atl_name="SC_SSPE_Planetary_Body_Exclusion">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_PBE_Resonances" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_PBE_Resonances" wwise_radius_max="80.000000"/>
			<ATLPreload atl_name="SC_SSPE_Planetary_Body_Exclusion"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_PBE_Warning_Beep" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_PBE_Warning_Beep"/>
			<ATLPreload atl_name="SC_SSPE_Planetary_Body_Exclusion"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_PBE_Resonances" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_PBE_Resonances" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_SSPE_Planetary_Body_Exclusion"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_PBE_Warning_Beep" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_PBE_Warning_Beep" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_SSPE_Planetary_Body_Exclusion"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_SSPE_Planetary_Body_Exclusion" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="SSPE_Planetary_Body_Exclusion.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
