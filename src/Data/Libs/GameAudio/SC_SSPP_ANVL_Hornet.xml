<ATLConfig atl_name="SC_SSPP_ANVL_Hornet">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_SSPP_ANVL_Hornet_Cockpit_Power" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_SSPP_ANVL_Hornet_Cockpit_Power"/>
			<ATLPreload atl_name="SC_SSPP_ANVL_Hornet"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_SSPP_ANVL_Hornet_Cockpit_Power" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_SSPP_ANVL_Hornet_Cockpit_Power" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_SSPP_ANVL_Hornet"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_SSPP_ANVL_Hornet" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="SSPP_ANVL_Hornet.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
