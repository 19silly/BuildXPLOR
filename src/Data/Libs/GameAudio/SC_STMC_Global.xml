<ATLConfig atl_name="SC_STMC_Global">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_LCMC_DistantMachine" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_LCMC_DistantMachine" wwise_radius_max="20.000000"/>
			<ATLPreload atl_name="SC_STMC_Global"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_LCMC_WaterProcessor" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_LCMC_WaterProcessor" wwise_radius_max="20.000000"/>
			<ATLPreload atl_name="SC_STMC_Global"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_LCNT_WaterDrip" path="Events/">
			<WwiseEvent wwise_duration_max="0.233586" wwise_duration_min="0.051581" wwise_duration_type="OneShot" wwise_name="Play_LCNT_WaterDrip" wwise_radius_max="20.000000"/>
			<ATLPreload atl_name="SC_STMC_Global"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_LCMC_DistantMachine" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_LCMC_DistantMachine" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_STMC_Global"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_LCMC_WaterProcessor" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_LCMC_WaterProcessor" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_STMC_Global"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_STMC_Global" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="STMC_Global.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
