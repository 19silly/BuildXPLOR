<ATLConfig atl_name="SC_PROP">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_PROP_Fabric_Tarp_Flap" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_PROP_Fabric_Tarp_Flap" wwise_radius_max="50.000000"/>
			<ATLPreload atl_name="SC_PROP"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_PROP_Hand_Fire_Extinguisher_Loop" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_PROP_Hand_Fire_Extinguisher_Loop" wwise_radius_max="20.000000"/>
			<ATLPreload atl_name="SC_PROP"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_PROP_Hand_Fire_Extinguisher_Puncture" path="Events/">
			<WwiseEvent wwise_duration_max="1.348141" wwise_duration_min="0.890317" wwise_duration_type="OneShot" wwise_name="Play_PROP_Hand_Fire_Extinguisher_Puncture" wwise_radius_max="20.000000"/>
			<ATLPreload atl_name="SC_PROP"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_PROP_Ventilation_Cover_Break" path="Events/">
			<WwiseEvent wwise_duration_max="1.720295" wwise_duration_min="1.530000" wwise_duration_type="OneShot" wwise_name="Play_PROP_Ventilation_Cover_Break" wwise_radius_max="20.000000"/>
			<ATLPreload atl_name="SC_PROP"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_PROP_Fabric_Tarp_Flap" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_PROP_Fabric_Tarp_Flap" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_PROP"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_PROP_Hand_Fire_Extinguisher_Loop" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_PROP_Hand_Fire_Extinguisher_Loop" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_PROP"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_PROP_Hand_Fire_Extinguisher_Puncture" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_PROP_Hand_Fire_Extinguisher_Puncture" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_PROP"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_PROP" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="PROP.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
