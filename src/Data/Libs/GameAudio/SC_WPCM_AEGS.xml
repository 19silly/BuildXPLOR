<ATLConfig atl_name="SC_WPCM_AEGS">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_WPCM_AEGS_Chaff_Explode" path="Events/">
			<WwiseEvent wwise_duration_max="4.583938" wwise_duration_min="4.212500" wwise_duration_type="OneShot" wwise_name="Play_WPCM_AEGS_Chaff_Explode" wwise_radius_max="1000.000000"/>
			<ATLPreload atl_name="SC_WPCM_AEGS"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_WPCM_AEGS_Chaff_Launch" path="Events/">
			<WwiseEvent wwise_duration_max="1.716187" wwise_duration_min="1.106375" wwise_duration_type="OneShot" wwise_name="Play_WPCM_AEGS_Chaff_Launch" wwise_radius_max="1000.000000"/>
			<ATLPreload atl_name="SC_WPCM_AEGS"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_WPCM_AEGS_Flare_Launch" path="Events/">
			<WwiseEvent wwise_duration_max="25.726349" wwise_duration_min="22.946844" wwise_duration_type="OneShot" wwise_name="Play_WPCM_AEGS_Flare_Launch" wwise_radius_max="10000.000000"/>
			<ATLPreload atl_name="SC_WPCM_AEGS"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_WPCM_AEGS_Flare_Burn" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_WPCM_AEGS_Flare_Burn" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_WPCM_AEGS"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_WPCM_AEGS" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="WPCM_AEGS.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
