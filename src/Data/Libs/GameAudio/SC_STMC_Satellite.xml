<ATLConfig atl_name="SC_STMC_Satellite">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_LCMC_Comms_Satellite_Power_Up" path="Events/">
			<WwiseEvent wwise_duration_max="7.800000" wwise_duration_min="7.800000" wwise_duration_type="OneShot" wwise_name="Play_LCMC_Comms_Satellite_Power_Up" wwise_radius_max="10.000000"/>
			<ATLPreload atl_name="SC_STMC_Satellite"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_STMC_Satellite" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="STMC_Satellite.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
