<ATLConfig atl_name="SC_WPHA_misc">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_WPHA_LR_HighRof_Fire" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_WPHA_LR_HighRof_Fire" wwise_radius_max="4700.000000"/>
			<ATLPreload atl_name="SC_WPHA_misc"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_WPHA_LR_HighRof_Fire" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_WPHA_LR_HighRof_Fire" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_WPHA_misc"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_WPHA_misc" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="WPHA_misc.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
