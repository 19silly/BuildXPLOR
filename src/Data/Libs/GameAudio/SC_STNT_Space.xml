<ATLConfig atl_name="SC_STNT_Space">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_LCNT_Space_Debris_Cloud" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_LCNT_Space_Debris_Cloud" wwise_radius_max="100.000000"/>
			<ATLPreload atl_name="SC_STNT_Space"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_LCNT_Space_Debris_Impact" path="Events/">
			<WwiseEvent wwise_duration_max="0.881677" wwise_duration_min="0.094410" wwise_duration_type="OneShot" wwise_name="Play_LCNT_Space_Debris_Impact" wwise_radius_max="100.000000"/>
			<ATLPreload atl_name="SC_STNT_Space"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_LCNT_Space_Debris_Cloud" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_LCNT_Space_Debris_Cloud" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_STNT_Space"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_STNT_Space" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="STNT_Space.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
