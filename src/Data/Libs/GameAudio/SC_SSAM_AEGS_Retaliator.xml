		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSAM_AEGS_Retaliator_Interior_Medium_Room" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_SSAM_AEGS_Retaliator_Interior_Medium_Room" wwise_radius_max="10.000000"/>
			<ATLPreload atl_name="SC_SSAM_AEGS_Retaliator"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSAM_AEGS_Retaliator_Interior_Small_Room" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_SSAM_AEGS_Retaliator_Interior_Small_Room" wwise_radius_max="10.000000"/>
			<ATLPreload atl_name="SC_SSAM_AEGS_Retaliator"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSMC_AEGS_Retaliator_Metal_Creak" path="Events/">
