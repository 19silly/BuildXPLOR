		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSMC_AEGS_Retaliator_Metal_Noise" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_SSMC_AEGS_Retaliator_Metal_Noise" wwise_radius_max="11.000000"/>
			<ATLPreload atl_name="SC_SSAM_AEGS_Retaliator"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSMC_AEGS_Retaliator_Vent_Airflow_High" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_SSMC_AEGS_Retaliator_Vent_Airflow_High" wwise_radius_max="10.000000"/>
			<ATLPreload atl_name="SC_SSAM_AEGS_Retaliator"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSMC_AEGS_Retaliator_Vent_Airflow_Low" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_SSMC_AEGS_Retaliator_Vent_Airflow_Low" wwise_radius_max="10.000000"/>
