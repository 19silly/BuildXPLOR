<ATLConfig atl_name="SC_SSAM_AEGS_Retaliator">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_SSAM_AEGS_Retaliator_Alarms" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_SSAM_AEGS_Retaliator_Alarms" wwise_radius_max="12.000000"/>
			<ATLPreload atl_name="SC_SSAM_AEGS_Retaliator"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSAM_AEGS_Retaliator_Bleep" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_SSAM_AEGS_Retaliator_Bleep" wwise_radius_max="10.000000"/>
