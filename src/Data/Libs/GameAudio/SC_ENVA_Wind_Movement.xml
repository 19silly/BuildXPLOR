<ATLConfig atl_name="SC_ENVA_Wind_Movement">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_ENVA_Int_Wind_Movement" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_ENVA_Int_Wind_Movement" wwise_radius_max="10.000000"/>
			<ATLPreload atl_name="SC_ENVA_Wind_Movement"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_ENVA_Int_Wind_Movement" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_ENVA_Int_Wind_Movement" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_ENVA_Wind_Movement"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_ENVA_Wind_Movement" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="ENVA_Wind_Movement.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
