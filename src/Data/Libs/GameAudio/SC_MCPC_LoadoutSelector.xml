<ATLConfig atl_name="SC_MCPC_LoadoutSelector">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_MCPC_LoadoutSelector_Operating_Hum" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_MCPC_LoadoutSelector_Operating_Hum" wwise_radius_max="30.000000"/>
			<ATLPreload atl_name="SC_MCPC_LoadoutSelector"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_MCPC_LoadoutSelector_Screen_Off" path="Events/">
			<WwiseEvent wwise_duration_max="0.693563" wwise_duration_min="0.693563" wwise_duration_type="OneShot" wwise_name="Play_MCPC_LoadoutSelector_Screen_Off" wwise_radius_max="30.000000"/>
			<ATLPreload atl_name="SC_MCPC_LoadoutSelector"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_MCPC_LoadoutSelector_Screen_On" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_MCPC_LoadoutSelector_Screen_On" wwise_radius_max="30.000000"/>
			<ATLPreload atl_name="SC_MCPC_LoadoutSelector"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_MCPC_LoadoutSelector_Operating_Hum" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_MCPC_LoadoutSelector_Operating_Hum" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_MCPC_LoadoutSelector"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_MCPC_LoadoutSelector" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="MCPC_LoadoutSelector.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
