<ATLConfig atl_name="SC_MCFB_Fabric">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_MCFB_Advert_Banner_Flap" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_MCFB_Advert_Banner_Flap" wwise_radius_max="125.000000"/>
			<ATLPreload atl_name="SC_MCFB_Fabric"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_MCFB_Tarp_Flap" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_MCFB_Tarp_Flap" wwise_radius_max="50.000000"/>
			<ATLPreload atl_name="SC_MCFB_Fabric"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_MCFB_Advert_Banner_Flap" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_MCFB_Advert_Banner_Flap" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_MCFB_Fabric"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_MCFB_Tarp_Flap" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_MCFB_Tarp_Flap" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_MCFB_Fabric"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_MCFB_Fabric" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="MCFB_Fabric.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
