<ATLConfig atl_name="SC_Environment_Ambience_Interiors">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_ENVA_Airlock_Ambience" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_ENVA_Airlock_Ambience" wwise_radius_max="10.000000"/>
			<ATLPreload atl_name="SC_Environment_Ambience_Interiors"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_ENVA_Bar_Counter_Amb" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_ENVA_Bar_Counter_Amb" wwise_radius_max="10.000000"/>
			<ATLPreload atl_name="SC_Environment_Ambience_Interiors"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_ENVA_Luxury_Lobby_Music" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_ENVA_Luxury_Lobby_Music" wwise_radius_max="10.000000"/>
			<ATLPreload atl_name="SC_Environment_Ambience_Interiors"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_ENVA_Luxury_Lobby_Tone" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_ENVA_Luxury_Lobby_Tone" wwise_radius_max="10.000000"/>
			<ATLPreload atl_name="SC_Environment_Ambience_Interiors"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_ENVA_Airlock_Ambience" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_ENVA_Airlock_Ambience" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_Environment_Ambience_Interiors"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_ENVA_Bar_Counter_Amb" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_ENVA_Bar_Counter_Amb" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_Environment_Ambience_Interiors"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_ENVA_Luxury_Lobby_Music" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_ENVA_Luxury_Lobby_Music" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_Environment_Ambience_Interiors"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_ENVA_Luxury_Lobby_Tone" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_ENVA_Luxury_Lobby_Tone" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_Environment_Ambience_Interiors"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_Environment_Ambience_Interiors" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="Environment_Ambience_Interiors.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
