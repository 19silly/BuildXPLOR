<ATLConfig atl_name="SC_STMC_TE_BattleArena">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_LCMC_TE_BattleArena_Adboard_Thruster" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_LCMC_TE_BattleArena_Adboard_Thruster" wwise_radius_max="120.000000"/>
			<ATLPreload atl_name="SC_STMC_TE_BattleArena"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_LCMC_TE_BattleArena_Rocket_Boost" path="Events/">
			<WwiseEvent wwise_duration_max="5.004989" wwise_duration_min="4.769728" wwise_duration_type="OneShot" wwise_name="Play_LCMC_TE_BattleArena_Rocket_Boost" wwise_radius_max="150.000000"/>
			<ATLPreload atl_name="SC_STMC_TE_BattleArena"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_LCMC_TE_BattleArena_Adboard_Thruster" path="Events/">
