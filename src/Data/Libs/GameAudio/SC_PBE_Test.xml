<ATLConfig atl_name="SC_PBE_Test">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_PBE_Resonances" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_PBE_Resonances"/>
			<ATLPreload atl_name="SC_PBE_Test"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_PBE_Resonances" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_PBE_Resonances" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_PBE_Test"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_PBE_Test" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="PBE_Test.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
