<ATLConfig atl_name="SC_SSES_Shared">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_SSES_Shared_Cockpit_Canopy_Eject" path="Events/">
			<WwiseEvent wwise_duration_max="2.205667" wwise_duration_min="2.205667" wwise_duration_type="OneShot" wwise_name="Play_SSES_Shared_Cockpit_Canopy_Eject" wwise_radius_max="350.000000"/>
			<ATLPreload atl_name="SC_SSES_Shared"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSES_Shared_Cockpit_Seat_Eject" path="Events/">
			<WwiseEvent wwise_duration_max="1.852894" wwise_duration_min="1.852894" wwise_duration_type="OneShot" wwise_name="Play_SSES_Shared_Cockpit_Seat_Eject" wwise_radius_max="350.000000"/>
			<ATLPreload atl_name="SC_SSES_Shared"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSES_Shared_Ejection_Alarm" path="Events/">
			<WwiseEvent wwise_duration_max="1.203764" wwise_duration_min="1.203764" wwise_duration_type="OneShot" wwise_name="Play_SSES_Shared_Ejection_Alarm" wwise_radius_max="250.000000"/>
			<ATLPreload atl_name="SC_SSES_Shared"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSES_Shared_Ejection_Button_Press" path="Events/">
			<WwiseEvent wwise_duration_max="0.146825" wwise_duration_min="0.146145" wwise_duration_type="OneShot" wwise_name="Play_SSES_Shared_Ejection_Button_Press" wwise_radius_max="250.000000"/>
			<ATLPreload atl_name="SC_SSES_Shared"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSES_Shared_Ejection_Handle_Pull" path="Events/">
			<WwiseEvent wwise_duration_max="0.355488" wwise_duration_min="0.355488" wwise_duration_type="OneShot" wwise_name="Play_SSES_Shared_Ejection_Handle_Pull" wwise_radius_max="250.000000"/>
			<ATLPreload atl_name="SC_SSES_Shared"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSES_Shared_Ejection_Launch" path="Events/">
