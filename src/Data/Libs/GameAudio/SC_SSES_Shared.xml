			<ATLPreload atl_name="SC_SSES_Shared"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSES_Shared_Stablization_Bursts" path="Events/">
			<WwiseEvent wwise_duration_max="3.486372" wwise_duration_min="3.486372" wwise_duration_type="OneShot" wwise_name="Play_SSES_Shared_Stablization_Bursts" wwise_radius_max="20.000000"/>
