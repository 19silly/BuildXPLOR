		</ATLTrigger>
		<ATLTrigger atl_name="Stop_SSES_Shared_Ejection_Launch" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_SSES_Shared_Ejection_Launch" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_SSES_Shared"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_SSES_Shared" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="SSES_Shared.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
