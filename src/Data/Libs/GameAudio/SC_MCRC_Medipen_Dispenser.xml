<ATLConfig atl_name="SC_MCRC_Medipen_Dispenser">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_medipen_dispenser_close" path="Events/">
			<WwiseEvent wwise_duration_max="0.926395" wwise_duration_min="0.926395" wwise_duration_type="OneShot" wwise_name="Play_medipen_dispenser_close" wwise_radius_max="30.000000"/>
			<ATLPreload atl_name="SC_MCRC_Medipen_Dispenser"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_medipen_dispenser_open" path="Events/">
			<WwiseEvent wwise_duration_max="0.750930" wwise_duration_min="0.750930" wwise_duration_type="OneShot" wwise_name="Play_medipen_dispenser_open" wwise_radius_max="30.000000"/>
			<ATLPreload atl_name="SC_MCRC_Medipen_Dispenser"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_MCRC_Medipen_Dispenser" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="MCRC_Medipen_Dispenser.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
