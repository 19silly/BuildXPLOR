<ATLConfig atl_name="SC_MCAV_Advertisements">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_MCAV_Murry_Cup_Tv_Advert" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_MCAV_Murry_Cup_Tv_Advert" wwise_radius_max="10.000000"/>
			<ATLPreload atl_name="SC_MCAV_Advertisements"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_MCAV_Murry_Cup_Tv_Advert" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_MCAV_Murry_Cup_Tv_Advert" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_MCAV_Advertisements"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_MCAV_Advertisements" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="MCAV_Advertisements.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
