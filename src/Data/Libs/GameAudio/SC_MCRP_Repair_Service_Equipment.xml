<ATLConfig atl_name="SC_MCRP_Repair_Service_Equipment">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_MCRP_Level_Stanton_Arccorp_Dumpers_Depot_Technical_Tap" path="Events/">
			<WwiseEvent wwise_duration_max="1.272441" wwise_duration_min="1.201024" wwise_duration_type="OneShot" wwise_name="Play_MCRP_Level_Stanton_Arccorp_Dumpers_Depot_Technical_Tap" wwise_radius_max="50.000000"/>
			<ATLPreload atl_name="SC_MCRP_Repair_Service_Equipment"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_MCRP_restocking_full" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_MCRP_restocking_full" wwise_radius_max="100.000000"/>
			<ATLPreload atl_name="SC_MCRP_Repair_Service_Equipment"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_MCRP_restocking_full_end" path="Events/">
			<WwiseEvent wwise_duration_max="2.804875" wwise_duration_min="2.804875" wwise_duration_type="OneShot" wwise_name="Play_MCRP_restocking_full_end" wwise_radius_max="100.000000"/>
			<ATLPreload atl_name="SC_MCRP_Repair_Service_Equipment"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_MCRP_restocking_full" path="Events/">
