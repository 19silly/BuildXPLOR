			<ATLPreload atl_name="SC_MCRP_Repair_Service_Equipment"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_MCRP_restocking_full_end" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_MCRP_restocking_full_end" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_MCRP_Repair_Service_Equipment"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_MCRP_Repair_Service_Equipment" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="MCRP_Repair_Service_Equipment.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
