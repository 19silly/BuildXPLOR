<ATLConfig atl_name="SC_NTAT_Atmospheric">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_NTAT_Atmosphere_Entry_Burn" path="Events/">
			<WwiseEvent wwise_duration_max="12.093597" wwise_duration_min="12.093597" wwise_duration_type="OneShot" wwise_name="Play_NTAT_Atmosphere_Entry_Burn" wwise_radius_max="1000.000000"/>
			<ATLPreload atl_name="SC_NTAT_Atmospheric"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_NTAT_Atmospheric_High_Winds" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_NTAT_Atmospheric_High_Winds" wwise_radius_max="100.000000"/>
			<ATLPreload atl_name="SC_NTAT_Atmospheric"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_NTAT_Atmospheric_High_Winds_2D" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_NTAT_Atmospheric_High_Winds_2D"/>
			<ATLPreload atl_name="SC_NTAT_Atmospheric"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_NTAT_Shockwave_Boom" path="Events/">
			<WwiseEvent wwise_duration_max="6.112000" wwise_duration_min="6.112000" wwise_duration_type="OneShot" wwise_name="Play_NTAT_Shockwave_Boom" wwise_radius_max="40.000000"/>
			<ATLPreload atl_name="SC_NTAT_Atmospheric"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_NTAT_Sonic_Booms" path="Events/">
			<WwiseEvent wwise_duration_max="7.336422" wwise_duration_min="7.336422" wwise_duration_type="OneShot" wwise_name="Play_NTAT_Sonic_Booms" wwise_radius_max="40.000000"/>
			<ATLPreload atl_name="SC_NTAT_Atmospheric"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_NTAT_Atmospheric_High_Winds" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_NTAT_Atmospheric_High_Winds" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_NTAT_Atmospheric"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_NTAT_Atmospheric_High_Winds_2D" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_NTAT_Atmospheric_High_Winds_2D" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_NTAT_Atmospheric"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_NTAT_Atmospheric" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="NTAT_Atmospheric.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
