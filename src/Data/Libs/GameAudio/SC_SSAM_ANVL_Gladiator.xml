<ATLConfig atl_name="SC_SSAM_ANVL_Gladiator">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_SSAM_ANVL_Gladiator_Interior_Cockpit" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_SSAM_ANVL_Gladiator_Interior_Cockpit" wwise_radius_max="10.000000"/>
			<ATLPreload atl_name="SC_SSAM_ANVL_Gladiator"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_SSAM_ANVL_Gladiator_Interior_Cockpit" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_SSAM_ANVL_Gladiator_Interior_Cockpit" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_SSAM_ANVL_Gladiator"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_SSAM_ANVL_Gladiator" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="SSAM_ANVL_Gladiator.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
