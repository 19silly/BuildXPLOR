<ATLConfig atl_name="SC_WPFA_RechargeStations">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_WPFA_Energy_Recharge" path="Events/">
			<WwiseEvent wwise_duration_max="4.195200" wwise_duration_min="4.195200" wwise_duration_type="OneShot" wwise_name="Play_WPFA_Energy_Recharge" wwise_radius_max="8.000000"/>
			<ATLPreload atl_name="SC_WPFA_RechargeStations"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_WPFA_FPS_Recharge_Loop" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_WPFA_FPS_Recharge_Loop" wwise_radius_max="8.000000"/>
			<ATLPreload atl_name="SC_WPFA_RechargeStations"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_WPFA_Energy_Recharge" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_WPFA_Energy_Recharge" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_WPFA_RechargeStations"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_WPFA_FPS_Recharge_Loop" path="Events/">
			<WwiseEvent wwise_duration_max="2.172354" wwise_duration_min="2.172354" wwise_duration_type="OneShot" wwise_name="Stop_WPFA_FPS_Recharge_Loop" wwise_radius_max="8.000000"/>
			<ATLPreload atl_name="SC_WPFA_RechargeStations"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_WPFA_RechargeStations" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="WPFA_RechargeStations.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
