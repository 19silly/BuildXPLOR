<ATLConfig atl_name="SC_CGGT_Hologram">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_CGGT_Hologram_Activate" path="Events/">
			<WwiseEvent wwise_duration_max="1.549777" wwise_duration_min="1.549777" wwise_duration_type="OneShot" wwise_name="Play_CGGT_Hologram_Activate" wwise_radius_max="20.000000"/>
			<ATLPreload atl_name="SC_CGGT_Hologram"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_CGGT_Hologram_Active" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_CGGT_Hologram_Active" wwise_radius_max="20.000000"/>
			<ATLPreload atl_name="SC_CGGT_Hologram"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_CGGT_Hologram_Deactivate" path="Events/">
			<WwiseEvent wwise_duration_max="1.930703" wwise_duration_min="1.930703" wwise_duration_type="OneShot" wwise_name="Play_CGGT_Hologram_Deactivate" wwise_radius_max="20.000000"/>
			<ATLPreload atl_name="SC_CGGT_Hologram"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_CGGT_Hologram_Select" path="Events/">
			<WwiseEvent wwise_duration_max="2.400000" wwise_duration_min="2.233333" wwise_duration_type="OneShot" wwise_name="Play_CGGT_Hologram_Select" wwise_radius_max="15.000000"/>
			<ATLPreload atl_name="SC_CGGT_Hologram"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_CGGT_Hologram_Active" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_CGGT_Hologram_Active" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_CGGT_Hologram"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_CGGT_Hologram" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="CGGT_Hologram.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
