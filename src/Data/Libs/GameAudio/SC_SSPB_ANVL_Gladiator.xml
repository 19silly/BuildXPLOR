<ATLConfig atl_name="SC_SSPB_ANVL_Gladiator">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_SSPB_ANVL_Gladiator_Close" path="Events/">
			<WwiseEvent wwise_duration_max="5.723312" wwise_duration_min="4.385500" wwise_duration_type="OneShot" wwise_name="Play_SSPB_ANVL_Gladiator_Close" wwise_radius_max="2000.000000"/>
			<ATLPreload atl_name="SC_SSPB_ANVL_Gladiator"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSPB_ANVL_Gladiator_Distant" path="Events/">
			<WwiseEvent wwise_duration_max="4.338750" wwise_duration_min="4.338750" wwise_duration_type="OneShot" wwise_name="Play_SSPB_ANVL_Gladiator_Distant" wwise_radius_max="2000.000000"/>
			<ATLPreload atl_name="SC_SSPB_ANVL_Gladiator"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSPB_ANVL_Gladiator_Traffic" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_SSPB_ANVL_Gladiator_Traffic" wwise_radius_max="1000.000000"/>
			<ATLPreload atl_name="SC_SSPB_ANVL_Gladiator"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_SSPB_ANVL_Gladiator_Traffic" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_SSPB_ANVL_Gladiator_Traffic" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_SSPB_ANVL_Gladiator"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_SSPB_ANVL_Gladiator" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="SSPB_ANVL_Gladiator.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
