<ATLConfig atl_name="SC_MCFL_Flair_Items">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_MCFL_Aquarium_Bubbles" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_MCFL_Aquarium_Bubbles" wwise_radius_max="5.000000"/>
			<ATLPreload atl_name="SC_MCFL_Flair_Items"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_MCFL_Calendar_Swipe" path="Events/">
			<WwiseEvent wwise_duration_max="0.533333" wwise_duration_min="0.533333" wwise_duration_type="OneShot" wwise_name="Play_MCFL_Calendar_Swipe" wwise_radius_max="5.000000"/>
			<ATLPreload atl_name="SC_MCFL_Flair_Items"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_MCFL_liquor_cabinet_close" path="Events/">
			<WwiseEvent wwise_duration_max="2.803958" wwise_duration_min="2.803771" wwise_duration_type="OneShot" wwise_name="Play_MCFL_liquor_cabinet_close" wwise_radius_max="20.000000"/>
			<ATLPreload atl_name="SC_MCFL_Flair_Items"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_MCFL_liquor_cabinet_drink" path="Events/">
			<WwiseEvent wwise_duration_max="8.270667" wwise_duration_min="8.270583" wwise_duration_type="OneShot" wwise_name="Play_MCFL_liquor_cabinet_drink" wwise_radius_max="10.000000"/>
			<ATLPreload atl_name="SC_MCFL_Flair_Items"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_MCFL_liquor_cabinet_open" path="Events/">
			<WwiseEvent wwise_duration_max="1.844164" wwise_duration_min="1.836342" wwise_duration_type="OneShot" wwise_name="Play_MCFL_liquor_cabinet_open" wwise_radius_max="20.000000"/>
			<ATLPreload atl_name="SC_MCFL_Flair_Items"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_MCFL_Aquarium_Bubbles" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_MCFL_Aquarium_Bubbles" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_MCFL_Flair_Items"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_MCFL_Flair_Items" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="MCFL_Flair_Items.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
