<ATLConfig atl_name="SC_CBPR">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_CBPR_EarDrumBurst_Loop" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_CBPR_EarDrumBurst_Loop" wwise_radius_max="32.000000"/>
			<ATLPreload atl_name="SC_CBPR"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_CBPR_EarDrumBurst_Loop" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_CBPR_EarDrumBurst_Loop" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_CBPR"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_CBPR" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="CBPR.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
