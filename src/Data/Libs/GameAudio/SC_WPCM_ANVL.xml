<ATLConfig atl_name="SC_WPCM_ANVL">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_WPCM_ANVL_Chaff_Explode" path="Events/">
			<WwiseEvent wwise_duration_max="4.583938" wwise_duration_min="4.212500" wwise_duration_type="OneShot" wwise_name="Play_WPCM_ANVL_Chaff_Explode" wwise_radius_max="1000.000000"/>
			<ATLPreload atl_name="SC_WPCM_ANVL"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_WPCM_ANVL_Chaff_Launch" path="Events/">
			<WwiseEvent wwise_duration_max="1.464503" wwise_duration_min="0.907999" wwise_duration_type="OneShot" wwise_name="Play_WPCM_ANVL_Chaff_Launch" wwise_radius_max="10000.000000"/>
			<ATLPreload atl_name="SC_WPCM_ANVL"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_WPCM_ANVL_Flare_Launch" path="Events/">
			<WwiseEvent wwise_duration_max="15.282993" wwise_duration_min="15.282993" wwise_duration_type="OneShot" wwise_name="Play_WPCM_ANVL_Flare_Launch" wwise_radius_max="1000.000000"/>
			<ATLPreload atl_name="SC_WPCM_ANVL"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_WPCM_ANVL_Flare_Burn" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_WPCM_ANVL_Flare_Burn" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_WPCM_ANVL"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_WPCM_ANVL" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="WPCM_ANVL.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
