		<ATLTrigger atl_name="Play_SSPB_AEGS_Gladius_600ms_0to100meters" path="Events/">
			<WwiseEvent wwise_duration_max="4.063084" wwise_duration_min="3.376848" wwise_duration_type="OneShot" wwise_name="Play_SSPB_AEGS_Gladius_600ms_0to100meters" wwise_radius_max="2000.000000"/>
			<ATLPreload atl_name="SC_SSPB_AEGS_Gladius"/>
