<ATLConfig atl_name="SC_CGGT_PAW">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_CGGT_PAW_Welder_hit" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_CGGT_PAW_Welder_hit" wwise_radius_max="20.000000"/>
			<ATLPreload atl_name="SC_CGGT_PAW"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_CGGT_PAW_Welder_Off" path="Events/">
			<WwiseEvent wwise_duration_max="2.912226" wwise_duration_min="1.943064" wwise_duration_type="OneShot" wwise_name="Play_CGGT_PAW_Welder_Off" wwise_radius_max="20.000000"/>
			<ATLPreload atl_name="SC_CGGT_PAW"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_CGGT_PAW_Welder_on" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_CGGT_PAW_Welder_on" wwise_radius_max="20.000000"/>
			<ATLPreload atl_name="SC_CGGT_PAW"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_CGGT_PAW_Welder_hit" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_CGGT_PAW_Welder_hit" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_CGGT_PAW"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_CGGT_PAW" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="CGGT_PAW.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
