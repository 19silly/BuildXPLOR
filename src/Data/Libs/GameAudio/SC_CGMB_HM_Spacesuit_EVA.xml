<ATLConfig atl_name="SC_CGMB_HM_Spacesuit_EVA">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_CGMB_Jetpack_Loop" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_CGMB_Jetpack_Loop" wwise_radius_max="15.000000"/>
			<ATLPreload atl_name="SC_CGMB_HM_Spacesuit_EVA"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_CGMB_Jetpack_Loop" path="Events/">
			<WwiseEvent wwise_duration_max="0.311931" wwise_duration_min="0.110040" wwise_duration_type="OneShot" wwise_name="Stop_CGMB_Jetpack_Loop" wwise_radius_max="15.000000"/>
			<ATLPreload atl_name="SC_CGMB_HM_Spacesuit_EVA"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_CGMB_HM_Spacesuit_EVA" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="CGMB_HM_Spacesuit_EVA.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
