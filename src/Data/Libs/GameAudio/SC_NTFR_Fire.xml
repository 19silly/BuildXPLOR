<ATLConfig atl_name="SC_NTFR_Fire">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_NTFR_Fire_Incinerater_Loop" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_NTFR_Fire_Incinerater_Loop" wwise_radius_max="20.000000"/>
			<ATLPreload atl_name="SC_NTFR_Fire"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_NTFR_Fire" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="NTFR_Fire.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
