<ATLConfig atl_name="SC_SSSG_Shared">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_SSSG_Shared_Human_Failing" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_SSSG_Shared_Human_Failing" wwise_radius_max="300.000000"/>
			<ATLPreload atl_name="SC_SSSG_Shared"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSSG_Shared_Human_Hit" path="Events/">
			<WwiseEvent wwise_duration_max="2.685102" wwise_duration_min="2.238186" wwise_duration_type="OneShot" wwise_name="Play_SSSG_Shared_Human_Hit" wwise_radius_max="300.000000"/>
			<ATLPreload atl_name="SC_SSSG_Shared"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSSG_Shared_Human_Regen" path="Events/">
			<WwiseEvent wwise_duration_max="3.515397" wwise_duration_min="3.515397" wwise_duration_type="OneShot" wwise_name="Play_SSSG_Shared_Human_Regen" wwise_radius_max="300.000000"/>
			<ATLPreload atl_name="SC_SSSG_Shared"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSSG_Shared_Vanduul_Hit" path="Events/">
			<WwiseEvent wwise_duration_max="2.685125" wwise_duration_min="2.685102" wwise_duration_type="OneShot" wwise_name="Play_SSSG_Shared_Vanduul_Hit" wwise_radius_max="300.000000"/>
			<ATLPreload atl_name="SC_SSSG_Shared"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSSG_Shared_Vanduul_Regen" path="Events/">
			<WwiseEvent wwise_duration_max="3.010181" wwise_duration_min="3.010181" wwise_duration_type="OneShot" wwise_name="Play_SSSG_Shared_Vanduul_Regen" wwise_radius_max="300.000000"/>
			<ATLPreload atl_name="SC_SSSG_Shared"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_SSSG_Shared_Human_Failing" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_SSSG_Shared_Human_Failing" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_SSSG_Shared"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_SSSG_Shared" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="SSSG_Shared.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
