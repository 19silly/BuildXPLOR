			<ATLPreload atl_name="SC_WPCM_ORIG"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_WPCM_ORIG" atl_type="AutoLoad" path="Banks/">
