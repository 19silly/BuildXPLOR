<ATLConfig atl_name="SC_NTWD_Wind">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_NTWD_Landing_Pad_Dustcloud" path="Events/">
			<WwiseEvent wwise_duration_max="25.589415" wwise_duration_min="25.589415" wwise_duration_type="OneShot" wwise_name="Play_NTWD_Landing_Pad_Dustcloud" wwise_radius_max="100.000000"/>
			<ATLPreload atl_name="SC_NTWD_Wind"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_NTWD_Landing_Pad_Winds" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_NTWD_Landing_Pad_Winds" wwise_radius_max="100.000000"/>
			<ATLPreload atl_name="SC_NTWD_Wind"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_NTWD_Wind_Through_Trees" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_NTWD_Wind_Through_Trees" wwise_radius_max="50.000000"/>
			<ATLPreload atl_name="SC_NTWD_Wind"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_NTWD_Landing_Pad_Winds" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_NTWD_Landing_Pad_Winds" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_NTWD_Wind"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_NTWD_Wind_Through_Trees" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_NTWD_Wind_Through_Trees" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_NTWD_Wind"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_NTWD_Wind" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="NTWD_Wind.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
