<ATLConfig atl_name="SC_NTWS_Weather_Systems">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_NTWS_Low_Thunder_Loop" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_NTWS_Low_Thunder_Loop" wwise_radius_max="40.000000"/>
			<ATLPreload atl_name="SC_NTWS_Weather_Systems"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_NTWS_Thunder_Oneshot" path="Events/">
			<WwiseEvent wwise_duration_max="8.892109" wwise_duration_min="5.801406" wwise_duration_type="OneShot" wwise_name="Play_NTWS_Thunder_Oneshot" wwise_radius_max="40.000000"/>
			<ATLPreload atl_name="SC_NTWS_Weather_Systems"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_NTWS_Low_Thunder_Loop" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_NTWS_Low_Thunder_Loop" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_NTWS_Weather_Systems"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_NTWS_Weather_Systems" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="NTWS_Weather_Systems.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
