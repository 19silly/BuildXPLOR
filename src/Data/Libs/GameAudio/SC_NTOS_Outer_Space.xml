<ATLConfig atl_name="SC_NTOS_Outer_Space">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_NTOS_Huge_Asteroid_Rumble" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_NTOS_Huge_Asteroid_Rumble" wwise_radius_max="6500.000000"/>
			<ATLPreload atl_name="SC_NTOS_Outer_Space"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_NTOS_Jumppoint_Exit" path="Events/">
			<WwiseEvent wwise_duration_max="9.418667" wwise_duration_min="9.418667" wwise_duration_type="OneShot" wwise_name="Play_NTOS_Jumppoint_Exit" wwise_radius_max="2000.000000"/>
			<ATLPreload atl_name="SC_NTOS_Outer_Space"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_NTOS_Outer_Space_2d" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_NTOS_Outer_Space_2d" wwise_radius_max="40.000000"/>
