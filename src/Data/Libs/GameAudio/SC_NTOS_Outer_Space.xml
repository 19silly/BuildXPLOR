		</ATLTrigger>
		<ATLTrigger atl_name="Play_NTOS_Space_Buoy__Beacon" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_NTOS_Space_Buoy__Beacon" wwise_radius_max="40.000000"/>
			<ATLPreload atl_name="SC_NTOS_Outer_Space"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_NTOS_Huge_Asteroid_Rumble" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_NTOS_Huge_Asteroid_Rumble" wwise_radius_max="0.000000"/>
