<ATLConfig atl_name="SC_NTAN_Animals">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_NTAN_Tree_Bird_Song" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_NTAN_Tree_Bird_Song" wwise_radius_max="50.000000"/>
			<ATLPreload atl_name="SC_NTAN_Animals"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_NTAN_Tree_Bird_Song" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_NTAN_Tree_Bird_Song" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_NTAN_Animals"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_NTAN_Animals" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="NTAN_Animals.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
