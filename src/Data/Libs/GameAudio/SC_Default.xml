<ATLConfig atl_name="SC_Default">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_Default_Event" path="Events/">
			<WwiseEvent wwise_duration_max="0.100000" wwise_duration_min="0.100000" wwise_duration_type="OneShot" wwise_name="Play_Default_Event"/>
			<ATLPreload atl_name="SC_Default"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_Default_Loop_Event" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_Default_Loop_Event"/>
			<ATLPreload atl_name="SC_Default"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_Deprecated_Event" path="Events/">
			<WwiseEvent wwise_duration_max="0.400000" wwise_duration_min="0.400000" wwise_duration_type="OneShot" wwise_name="Play_Deprecated_Event"/>
			<ATLPreload atl_name="SC_Default"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_Placeholder_Event" path="Events/">
			<WwiseEvent wwise_duration_max="0.050000" wwise_duration_min="0.050000" wwise_duration_type="OneShot" wwise_name="Play_Placeholder_Event"/>
			<ATLPreload atl_name="SC_Default"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_Default_Loop_Event" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_Default_Loop_Event" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_Default"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_Placeholder_Event" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_Placeholder_Event" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_Default"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_Default" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="Default.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
