<ATLConfig atl_name="SC_SSPB_ORIG_300">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_SSPB_ORIG_300_2500ms_0to200meters" path="Events/">
			<WwiseEvent wwise_duration_max="8.666667" wwise_duration_min="6.100000" wwise_duration_type="OneShot" wwise_name="Play_SSPB_ORIG_300_2500ms_0to200meters" wwise_radius_max="2000.000000"/>
			<ATLPreload atl_name="SC_SSPB_ORIG_300"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSPB_ORIG_300_PassBy_800ms_0to100mtrs" path="Events/">
			<WwiseEvent wwise_duration_max="4.960000" wwise_duration_min="3.466667" wwise_duration_type="OneShot" wwise_name="Play_SSPB_ORIG_300_PassBy_800ms_0to100mtrs" wwise_radius_max="2000.000000"/>
			<ATLPreload atl_name="SC_SSPB_ORIG_300"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSPB_ORIG_300_PassBy_Loop" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_SSPB_ORIG_300_PassBy_Loop" wwise_radius_max="2200.000000"/>
			<ATLPreload atl_name="SC_SSPB_ORIG_300"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSPB_ORIG_300_Traffic" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_SSPB_ORIG_300_Traffic" wwise_radius_max="1000.000000"/>
			<ATLPreload atl_name="SC_SSPB_ORIG_300"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_SSPB_ORIG_300_PassBy_Loop" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_SSPB_ORIG_300_PassBy_Loop" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_SSPB_ORIG_300"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_SSPB_ORIG_300_Traffic" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_SSPB_ORIG_300_Traffic" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_SSPB_ORIG_300"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_SSPB_ORIG_300" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="SSPB_ORIG_300.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
