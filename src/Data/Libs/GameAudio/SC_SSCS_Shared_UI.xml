		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSCS_Radiation_Warning_Loop_02" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_SSCS_Radiation_Warning_Loop_02" wwise_radius_max="10.000000"/>
