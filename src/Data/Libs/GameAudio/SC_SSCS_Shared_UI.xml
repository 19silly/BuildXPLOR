<ATLConfig atl_name="SC_SSCS_Shared_UI">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_SSCS_Landing_UI_Orientation_Feedback" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_SSCS_Landing_UI_Orientation_Feedback"/>
			<ATLPreload atl_name="SC_SSCS_Shared_UI"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSCS_Radiation_Alert" path="Events/">
			<WwiseEvent wwise_duration_max="1.960750" wwise_duration_min="1.960750" wwise_duration_type="OneShot" wwise_name="Play_SSCS_Radiation_Alert" wwise_radius_max="10.000000"/>
			<ATLPreload atl_name="SC_SSCS_Shared_UI"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_SSCS_Radiation_Alert_High" path="Events/">
