	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_SSCS_Shared_UI" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="SSCS_Shared_UI.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
