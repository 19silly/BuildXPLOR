<ATLConfig atl_name="SC_MCCS_RadioStatic">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_MCCS_RadioStatic" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_MCCS_RadioStatic" wwise_radius_max="8.000000"/>
			<ATLPreload atl_name="SC_MCCS_RadioStatic"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_MCCS_RadioStatic" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_MCCS_RadioStatic" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_MCCS_RadioStatic"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_MCCS_RadioStatic" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="MCCS_RadioStatic.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
