<ATLConfig atl_name="SC_NTWT_Water">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_NTWT_Water_Fish_Tank_Bubbles" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_NTWT_Water_Fish_Tank_Bubbles" wwise_radius_max="7.000000"/>
			<ATLPreload atl_name="SC_NTWT_Water"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_NTWT_Water_Fish_Tank_Bubbles" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_NTWT_Water_Fish_Tank_Bubbles" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_NTWT_Water"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_NTWT_Water" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="NTWT_Water.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
