<ATLConfig atl_name="SC_SSDD_RSI_Merlin">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_SSDD_RSI_Merlin_Cockpit_Shake" path="Events/">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_SSDD_RSI_Merlin_Cockpit_Shake"/>
			<ATLPreload atl_name="SC_SSDD_RSI_Merlin"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_SSDD_RSI_Merlin_Cockpit_Shake" path="Events/">
			<WwiseEvent wwise_duration_max="0.000000" wwise_duration_min="0.000000" wwise_duration_type="OneShot" wwise_name="Stop_SSDD_RSI_Merlin_Cockpit_Shake" wwise_radius_max="0.000000"/>
			<ATLPreload atl_name="SC_SSDD_RSI_Merlin"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_SSDD_RSI_Merlin" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc"/>
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="SSDD_RSI_Merlin.bnk"/>
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>
