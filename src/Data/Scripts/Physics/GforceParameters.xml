  <accel_dir_rate value="8.0"/>
  <!-- accel_mag_rate = Acceleration magnitude change rate -->
  <accel_mag_rate value="5.0"/>
  <!-- jitter_period = Time between updates in random jitter value -->
  <jitter_period value="0.01"/>
  <!-- jitter_smoothing = Speed of change from one jitter value to the next -->
  <jitter_smoothing value="200.0"/>
