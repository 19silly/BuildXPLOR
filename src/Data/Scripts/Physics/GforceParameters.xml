<GforceParameters version="1">
  <!-- PLEASE TALK TO JOHN PRITCHETT BEFORE CHANGING THESE VALUES -->
  <!-- body_k = Pilot body spring constant -->
  <body_k value="7000.0"/>
  <!-- body_k_change = Pilot body muscle response speed (higher is faster) -->
  <body_k_change value="0.5"/>
  <!-- body_b = Pilot body damping constant -->
  <body_b value="400.0"/>
  <!-- body_zetafactor = Pilot body damping ratio -->
  <body_zetafactor value="0.07"/>
  <!-- body_omegafactor = Pilot body natural frequency -->
  <body_omegafactor value="0.1"/>
  <!-- body_x_low = Pilot body forward max angle (degrees) -->
  <body_x_low value="-30.0"/>
  <!-- body_x_high = Pilot body backward max angle (degrees) -->
  <body_x_high value="30.0"/>
  <!-- body_y_low = Pilot body left max angle (degrees) -->
  <body_y_low value="-30.0"/>
  <!-- body_y_high = Pilot body right max angle (degrees) -->
  <body_y_high value="30.0"/>
  <!-- head_k = Pilot head spring constant -->
  <head_k value="90.0"/>
  <!-- head_k_change = Pilot neck muscle response speed (higher is faster) -->
  <head_k_change value="2.0"/>
  <!-- head_b = Pilot head damping constant -->
  <head_b value="35.0"/>
  <!-- head_zetafactor = Pilot head damping ratio -->
  <head_zetafactor value="2.0"/>
