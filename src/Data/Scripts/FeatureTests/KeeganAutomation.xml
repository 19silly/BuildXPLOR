<FeatureTester>
    
    <Tests setName="Hornet_Strafe_Test">
		<FeatureTest name="Hornet_Strafe_Test" enabled="true" require="localPlayerExists">
        
			<DoConsoleCommand command="ft_map_forceRun Hornet_Strafe" />
            
            <SetResponseToHittingCCCPoint checkpointName="FlowGraphFeatureTestSuccess" response="expectedNext" />
            <SetResponseToHittingCCCPoint checkpointName="FlowGraphFeatureTestFailure" response="failTest" />
            <WaitUntilHitAllExpectedCCCPoints timeOut="30" />
            
		</FeatureTest>
	</Tests>
