<Loadout>
	<Pipes>
		<Pipe name="BatteryPower" class="Power" priority="1"/>
		<Pipe name="MainPower" class="Power" priority="0"/>
		<Pipe name="Avionics" class="Avionics" type="1" />
		<Pipe name="Heat" class="Heat"  />
		<Pipe name="MainFuel" class="Fuel" priority="1" />
		<Pipe name="BatteryFuel" class="Fuel" priority="0" />
	</Pipes>
	
	<VehicleItems>
		
		<VehicleItem portName="weapon_mount" itemName="Class_2b_Placeholder">
			<Pipes>
				<Connection pipeClass="Power" pipe="MainPower" prio0="0"  prio1="10"  prio2="99" />
				<Connection pipeClass="Heat" pipe="Heat" prio0="0"  prio1="10"  prio2="99" />
			</Pipes>
			<Items>
				<VehicleItem portName="hardpoint_class_2" itemName="BEHR_LC_S1_Q1" >
				<Items>
					<VehicleItem portName="ammo_box_01" itemName="AmmoBox_Ballistic_20mm" />
					<VehicleItem portName="ammo_box_02" itemName="AmmoBox_Ballistic_20mm" />
				</Items>
				</VehicleItem>
			</Items>
		</VehicleItem>
		
		<VehicleItem portName="weapon_mount_top" itemName="Class_2b_Placeholder">
			<Pipes>
				<Connection pipeClass="Power" pipe="MainPower" prio0="0"  prio1="10"  prio2="99" />
				<Connection pipeClass="Heat" pipe="Heat" prio0="0"  prio1="10"  prio2="99" />
			</Pipes>
			<Items>
				<VehicleItem portName="hardpoint_class_2" itemName="BEHR_LC_S1_Q1" >
				<Items>
					<VehicleItem portName="ammo_box_01" itemName="AmmoBox_Ballistic_20mm" />
					<VehicleItem portName="ammo_box_02" itemName="AmmoBox_Ballistic_20mm" />
				</Items>
				</VehicleItem>
			</Items>
		</VehicleItem>
		
		<VehicleItem portName="weapon_mount_right" itemName="Class_2b_Placeholder">
			<Pipes>
				<Connection pipeClass="Power" pipe="MainPower" prio0="0"  prio1="10"  prio2="99" />
				<Connection pipeClass="Heat" pipe="Heat" prio0="0"  prio1="10"  prio2="99" />
			</Pipes>
			<Items>
				<VehicleItem portName="hardpoint_class_2" itemName="BEHR_LC_S1_Q1" >
				<Items>
					<VehicleItem portName="ammo_box_01" itemName="AmmoBox_Ballistic_20mm" />
					<VehicleItem portName="ammo_box_02" itemName="AmmoBox_Ballistic_20mm" />
				</Items>
				</VehicleItem>
			</Items>
		</VehicleItem>
		
		<VehicleItem portName="weapon_mount_left" itemName="Class_2b_Placeholder">
			<Pipes>
				<Connection pipeClass="Power" pipe="MainPower" prio0="0"  prio1="10"  prio2="99" />
				<Connection pipeClass="Heat" pipe="Heat" prio0="0"  prio1="10"  prio2="99" />
			</Pipes>
			<Items>
				<VehicleItem portName="hardpoint_class_2" itemName="BEHR_LC_S1_Q1" >
				<Items>
					<VehicleItem portName="ammo_box_01" itemName="AmmoBox_Ballistic_20mm" />
					<VehicleItem portName="ammo_box_02" itemName="AmmoBox_Ballistic_20mm" />
				</Items>
				</VehicleItem>
			</Items>
		</VehicleItem>
		
		
		
		
		
		<VehicleItem portName="hardpoint_avonics_motherboard" itemName="RSI_DefaultMotherboard">
			<Pipes>
				<Connection pipeClass="Power" pipe="MainPower" prio0="0"  prio1="10"  prio2="99" />
				<Connection pipeClass="Heat" pipe="Heat" prio0="0"  prio1="10"  prio2="99" />
				<Connection pipeClass="Avionics" pipe="Avionics" prio0="0"  prio1="10"  prio2="99" />
			</Pipes>
			<Items>
					<VehicleItem portName="hardpoint_cpu_slot" itemName="RSI_DefaultCPU" />
					<VehicleItem portName="hardpoint_targeting_computer" itemName="RSI_DefaultTargetSelector" />
					<VehicleItem portName="hardpoint_ads_computer" itemName="RSI_DefaultADS" />
					<VehicleItem portName="hardpoint_weapon_control" itemName="RSI_DefaultWeaponControl" />
			</Items>
		</VehicleItem>

		<VehicleItem portName="hardpoint_power_plant_attach" itemName="LPLT_SQV_S2_Q3">
			<Pipes>
				<Connection pipeClass="Heat" pipe="Heat" prio0="0"  prio1="10"  prio2="99" />
				<Connection pipeClass="Power" pipe="MainPower" prio0="0"  prio1="10"  prio2="99" />
				<Connection pipeClass="Power" pipe="BatteryPower" prio0="0"  prio1="10"  prio2="99" />
				<!-- Specific Pipe I/O for batteries -->
				<Connection pipeClass="Input" pipe="BatteryPower" prio0="0"  prio1="10"  prio2="99" />
				<Connection pipeClass="Output" pipe="MainPower" prio0="0"  prio1="10"  prio2="99" />
			</Pipes>
			<Items>
					<VehicleItem portName="hardpoint_battery" itemName="RSI_DefaultBattery" />
			</Items>
		</VehicleItem>
		
		<VehicleItem portName="hardpoint_radiator_attach" itemName="RSI_DefaultCooler">
			<Pipes>
				<Connection pipeClass="Heat" pipe="Heat" prio0="0"  prio1="10"  prio2="99" />
			</Pipes>
		</VehicleItem>

		
		
		
	</VehicleItems>
</Loadout>
