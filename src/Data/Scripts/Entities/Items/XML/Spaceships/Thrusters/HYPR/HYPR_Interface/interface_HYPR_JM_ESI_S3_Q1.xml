<interface name="interface_HYPR_JM_ESI_S3_Q1">
	<!-- 
	This is the model and vfx template for Thrusters.
	Match Entity Name to corresponding item.xml
	Fill damagelevels, geometry and exhausts to complete.
	-->
	<params>
		<param name="giveable" 			value="0" />
		<param name="selectable" 		value="0" />
		<param name="pickable" 			value="0" />
		<param name="mountable" 		value="1" />
		<param name="usable"		 	value="0" />
		<param name="arms"			 	value="0" />
		<!-- ONLY use if Thruster has special one-off params. All other params should remain in item.xml. -->
	</params>



  <damagelevels>
	<level damageRatio="0.50">
		<DebrisList>
			<Debris name="Ball_Thruster_50_debris1" mass="10" impulse="0,10,0" >
				<EffectsList>
						<Effect effect="damage_fx.uee.debris.debris_fire_small"  />
				</EffectsList>
			</Debris>
			<Debris name="Ball_Thruster_50_debris2" mass="10" impulse="0,10,0" >
				<EffectsList>
						<Effect effect="damage_fx.uee.static.electric_small"  />
				</EffectsList>
			</Debris>
		</DebrisList>
	</level>
	<level damageRatio="1.00">
		<DebrisList>
			<Debris name="Ball_Thruster_100_debris1" mass="10" impulse="0,10,0" >
				<EffectsList>
						<Effect effect="damage_fx.uee.debris.debris_fire_small"  />
				</EffectsList>
			</Debris>
			<Debris name="Ball_Thruster_100_debris2" mass="10" impulse="0,10,0" >
				<EffectsList>
						<Effect effect="damage_fx.uee.static.electric_small"  />
				</EffectsList>
			</Debris>
		</DebrisList>
	</level>
  </damagelevels>


  <geometry>
    <thirdperson name="Objects/Spaceships/Thrusters/ORIG/Scalpel_Precision/Origin_Precision_Omni_Thruster.cga"/>
		<tags tag="damage_50">
			<thirdperson name="Objects/Spaceships/Thrusters/ORIG/Scalpel_Precision/Origin_Precision_Omni_Thruster_50.cga"/>
		</tags>
		<tags tag="damage_100">
			<thirdperson name="Objects/Spaceships/Thrusters/ORIG/Scalpel_Precision/Origin_Precision_Omni_Thruster_100.cga"/>
		</tags>
	
		<tags tag="Hologram">
	
			<thirdperson name="Objects/Spaceships/Thrusters/ORIG/Scalpel_Precision/Origin_Precision_Omni_Thruster.cga" material=""/>

			<tags tag="ShipHologram">
				<thirdperson name="Objects/Spaceships/Thrusters/ORIG/Scalpel_Precision/Origin_Precision_Omni_Thruster.cga" material=""/>
		  
				<tags tag="inactive">
					<thirdperson name="Objects/Spaceships/Thrusters/ORIG/Scalpel_Precision/Origin_Precision_Omni_Thruster.cga" material=""/>
				</tags>
				<tags tag="over dragging">
				<thirdperson name="Objects/Spaceships/Thrusters/ORIG/Scalpel_Precision/Origin_Precision_Omni_Thruster.cga" material=""/>
				</tags>
				<tags tag="overCompareValid">
					<thirdperson name="Objects/Spaceships/Thrusters/ORIG/Scalpel_Precision/Origin_Precision_Omni_Thruster.cga" material=""/>
				</tags>
				<tags tag="overCompareInvalid">
					<thirdperson name="Objects/Spaceships/Thrusters/ORIG/Scalpel_Precision/Origin_Precision_Omni_Thruster.cga" material=""/>
				</tags>
			</tags>
		
			<tags tag="ItemList">
				<thirdperson name="Objects/Spaceships/Thrusters/ORIG/Scalpel_Precision/Origin_Precision_Omni_Thruster.cga" material=""/>
				<tags tag="over dragging">
					<thirdperson name="Objects/Spaceships/Thrusters/ORIG/Scalpel_Precision/Origin_Precision_Omni_Thruster.cga" material=""/>
				</tags>
			</tags>	
		
			<tags tag="ItemPreviewA">
		  	<thirdperson name="Objects/Spaceships/Thrusters/ORIG/Scalpel_Precision/Origin_Precision_Omni_Thruster.cga" material=""/>
			</tags>
		
			<tags tag="ItemPreviewB">
		 	 <thirdperson name="Objects/Spaceships/Thrusters/ORIG/Scalpel_Precision/Origin_Precision_Omni_Thruster.cga" material=""/>
			</tags>	
		
		</tags>
		
		<tags tag="HUD">
			<thirdperson name="Objects\UI\Thrusters\ORIG\Scalpel_Precision\Origin_Precision_Omni_Thruster_UI.cgf" material="materials/item_selection/material_hologram_default.mtl"/>
			<tags tag="damage_50">
				<thirdperson name="Objects\UI\Thrusters\ORIG\Scalpel_Precision\Origin_Precision_Omni_Thruster_UI.cgf" material="Materials\item_selection\material_hologram_mid_damaged.mtl"/>
			</tags>
			<tags tag="damage_100">
				<thirdperson name="Objects\UI\Thrusters\ORIG\Scalpel_Precision\Origin_Precision_Omni_Thruster_UI.cgf" material="Materials\item_selection\material_hologram_damaged.mtl"/>
			</tags>	
		</tags>	
		
	<boneAttachments>
		<attachment target="thirdperson" name="nozzle_exhaust" bone="exhaust" />
    </boneAttachments>		
  </geometry>
  
  <thrusters>
	<thruster>
		<exhausts>
				<exhaust name ="run" scale ="1" helper="nozzle_exhaust" effectStopThreshold="0.1" >
					<effect name="thruster_fx.Templates.Thrust-Maneuver-06-Joint" helper="nozzle_exhaust" enable="0" tag ="run">
						<strength var="thrust" />
						<speed var="speed" multiplier="3" />	
					</effect>
					<sound name="sounds/thrusters:orig_omni_precision_ball:start_run" enable ="0" tag = "run" loop="0">
						<param name="rpm" var="thrust" lerpTime="1" />
						<param name="boost" var="boost" lerpTime="2" />
						<param name="in_out" var="in_out" />
					</sound>
					<sound name="sounds/thrusters:orig_omni_precision_ball:stop" enable ="0" tag = "stop" loop="0">
						<param name="in_out" var="in_out" />
						<param name="rpm" var="thrust" lerpTime="0.75" />
						<param name="boost" var="boost" lerpTime="2" />
					</sound>
					<!--<param name="load" var="load" min="0.2" lerpTime="1" />-->
				</exhaust>
		</exhausts>
	  </thruster>
  </thrusters>

</interface>
